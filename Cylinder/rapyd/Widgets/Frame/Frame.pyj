@external
class Widget:
    pass

define(["Widget"], def(Widget):
    class Frame(Widget):
        def __init__(self, selector):
            Widget.__init__(self, None, None)
            self.$element = $(selector)
            self.$element.resize(self.onResize.bind(self))

        def onResize(self):
            self.notify(self.RESIZE, self.$element.width(), self.$element.height())


    return Frame
)